# PostgreSQL Configuration
POSTGRES_DB=n8n
POSTGRES_USER=postgres
POSTGRES_PASSWORD=ChangeThisToYourSecurePassword
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=ChangeThisToYourSecurePassword

# n8n Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=ChangeThisToYourSecurePassword
N8N_HOST=localhost
WEBHOOK_URL=http://localhost:5678/
GENERIC_TIMEZONE=America/New_York
N8N_ENCRYPTION_KEY=Generate32CharacterRandomEncryptionKey

# pgAdmin Configuration (Optional - only needed when using pgAdmin)
PGADMIN_EMAIL=your-email@example.com
PGADMIN_PASSWORD=ChangeThisToYourSecurePassword

# AI Integration (For future use with Ollama)
OLLAMA_HOST=http://host.docker.internal:11434
OLLAMA_MODEL=phi3:mini

# Application Settings
NODE_ENV=production
LOG_LEVEL=info

# Security Settings
N8N_SECURE_COOKIE=false
N8N_DISABLE_PRODUCTION_MAIN_PROCESS=false

# Feature Flags
N8N_RUNNERS_ENABLED=true
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true

# Custom Settings
N8N_PERSONALIZATION_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=false
N8N_DIAGNOSTICS_ENABLED=false
N8N_ANONYMOUS_USAGE_TRACKING=false
